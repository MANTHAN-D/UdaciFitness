{
  "expo": {
    "name": "udacifitness",
    "description": "An app to track triathlon trainning.",
    "slug": "udacifitness",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "monty.udacifitness.exp"
    },
    "android": {
      "package": "monty.udacifitness.exp"
    }
  }
}
